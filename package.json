{
  "name": "pattern-library",
  "version": "1.0.0",
  "description": "UI pattern library and living style guide for Private Acre.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "sass:core": "node-sass scss/pa-core.scss css/pa-core.min.css --output-style compressed",
    "sass:app": "node-sass scss/app.scss css/app.min.css --output-style compressed",
    "sass": "npm run sass:core && npm run sass:app",
    "watch:core": "node-sass -w scss/pa-core.scss css/pa-core.css",
    "watch:app": "node-sass -w scss/app.scss css/app.css",
    "build": "node-sass scss -o css --source-maps",
    "deploy": "gh-pages -d ./ -b gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/privateacre/pattern-library.git"
  },
  "keywords": [],
  "author": "Adam Penly",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/privateacre/pattern-library/issues"
  },
  "homepage": "https://github.com/privateacre/pattern-library",
  "devDependencies": {
    "node-sass": "^4.5.3",
    "sass-flex-mixin": "^1.0.3"
  },
  "dependencies": {}
}